 <id>http://davedash.com/</id>
 <author>
   <name>Dave Dash</name>
   <email>dd+atom1@davedash.com</email>
 </author>

 {% for post in site.tags['sfGuarduser'] %}
 <entry>
   <title>{{ post.title }}</title>
